<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Styled UI Template</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/css/bootstrap.min.css" rel="stylesheet">
</head>
<body>
    <div class="container py-4">
        <!-- Horizontal Spacer -->
        <div class="row">
            <div class="col-12 mb-3">
                <div class="bg-light p-2">Horizontal Spacer</div>
            </div>
        </div>

        <!-- Vertical Spacer -->
        <div class="row mb-3">
            <div class="col-12">
                <div class="bg-light p-2" style="height: 50px;">Vertical Spacer</div>
            </div>
        </div>

        <!-- Panel -->
        <div class="row mb-3">
            <div class="col-12">
                <div class="border p-3">Panel</div>
            </div>
        </div>

        <!-- Scroll Pane -->
        <div class="row mb-3">
            <div class="col-12">
                <div class="border p-3" style="overflow-y: scroll; height: 150px;">Scroll Pane</div>
            </div>
        </div>

        <!-- Button -->
        <div class="row mb-3">
            <div class="col-12">
                <button class="btn btn-primary">Button</button>
            </div>
        </div>

        <!-- Radio Button -->
        <div class="row mb-3">
            <div class="col-12">
                <div class="form-check">
                    <input class="form-check-input" type="radio" id="radioButton">
                    <label class="form-check-label" for="radioButton">RadioButton</label>
                </div>
            </div>
        </div>

        <!-- Check Box -->
        <div class="row mb-3">
            <div class="col-12">
                <div class="form-check">
                    <input class="form-check-input" type="checkbox" id="checkBox">
                    <label class="form-check-label" for="checkBox">CheckBox</label>
                </div>
            </div>
        </div>

        <!-- Label -->
        <div class="row mb-3">
            <div class="col-12">
                <label for="textField" class="form-label">Label</label>
            </div>
        </div>

        <!-- Text Field -->
        <div class="row mb-3">
            <div class="col-12">
                <input type="text" class="form-control" id="textField" placeholder="Enter text">
            </div>
        </div>

        <!-- Password Field -->
        <div class="row mb-3">
            <div class="col-12">
                <input type="password" class="form-control" id="passwordField" placeholder="Enter password">
            </div>
        </div>

        <!-- Formatted Text Field -->
        <div class="row mb-3">
            <div class="col-12">
                <input type="text" class="form-control" id="formattedTextField" placeholder="Formatted text">
            </div>
        </div>

        <!-- Text Area -->
        <div class="row mb-3">
            <div class="col-12">
                <textarea class="form-control" id="textArea" rows="3" placeholder="Text Area"></textarea>
            </div>
        </div>

        <!-- Combo Box -->
        <div class="row mb-3">
            <div class="col-12">
                <select class="form-select" id="comboBox">
                    <option selected>Select an option</option>
                    <option value="1">Option 1</option>
                    <option value="2">Option 2</option>
                </select>
            </div>
        </div>

        <!-- Progress Bar -->
        <div class="row mb-3">
            <div class="col-12">
                <div class="progress">
                    <div class="progress-bar" role="progressbar" style="width: 50%;" aria-valuenow="50" aria-valuemin="0" aria-valuemax="100">50%</div>
                </div>
            </div>
        </div>

        <!-- Slider -->
        <div class="row mb-3">
            <div class="col-12">
                <input type="range" class="form-range" id="slider">
            </div>
        </div>
    </div>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.bundle.min.js"></script>
</body>
</html>
